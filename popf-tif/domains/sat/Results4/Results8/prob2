(:init
  (= (n-pattern) 0)
  (= (expected-time) 0)
  (= (previous-expected-time) 0)
  (= (total-probability) 0)
  (= (previous-total-probability) 0)
  (= (heuristic-approximation) 0.001)
  (at origin)
  (= (previous-probability v-885726) 0.33333334)
  (= (probability v-885726) 0.33333334)
  (= (previous-probability v-897381) 0.33333334)
  (= (probability v-897381) 0.33333334)
  (= (previous-probability v-903747) 0.33333334)
  (= (probability v-903747) 0.33333334)
  (= (is-doing spiral3) 0)
  (= (timefor spiral3) 299)
  (beginAt s3s spiral3)
  (endAt s3e spiral3)
  (at 642 (active spiral3))
  (at 1408 (not (active spiral3)))
  (= (n-pattern-active spiral3) 1025.0)
 (= (distance origin s3s) 18)
  (= (distance s3e s3s) 38)
  (= (distance s3e s4s) 65)
  (= (distance s3e s5s) 153)
  (= (is-doing spiral4) 0)
  (= (timefor spiral4) 299)
