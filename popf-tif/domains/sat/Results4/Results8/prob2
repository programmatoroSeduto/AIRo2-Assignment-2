  (= (distance s5e s3s) 209)
  (= (distance s5e s4s) 108)
  (= (distance s5e s5s) 38)
)
(:goal (and (> (total-probability) 0)))
(:metric maximize ( - (total-probability) (* 0.1 (expected-time)))))
